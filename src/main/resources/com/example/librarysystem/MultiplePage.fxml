<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarysystem.MultiplePageController">
    <children>
        <BorderPane prefHeight="400.0" prefWidth="614.0">
            <left>
                <VBox prefHeight="400.0" prefWidth="166.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="BooksBtn" mnemonicParsing="false" onAction="#OpenBooks" prefHeight="38.0" prefWidth="103.0" text="Books">
                            <VBox.margin>
                                <Insets left="20.0" top="20.0" />
                            </VBox.margin></Button>
                        <Button fx:id="MembersBtn" mnemonicParsing="false" onAction="#OpenMembers" prefHeight="43.0" prefWidth="101.0" text="Members">
                            <VBox.margin>
                                <Insets left="20.0" top="20.0" />
                            </VBox.margin></Button>
                        <Button mnemonicParsing="false" prefHeight="43.0" prefWidth="101.0" text="Button">
                            <VBox.margin>
                                <Insets left="20.0" top="20.0" />
                            </VBox.margin></Button>
                        <Button fx:id="RulesBtn" mnemonicParsing="false" onAction="#OpenRules" prefHeight="38.0" prefWidth="103.0" text="Rules">
                            <VBox.margin>
                                <Insets left="20.0" top="20.0" />
                            </VBox.margin></Button>
                        <Button mnemonicParsing="false" prefHeight="14.0" prefWidth="102.0" text="Button">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                            <VBox.margin>
                                <Insets left="20.0" top="20.0" />
                            </VBox.margin></Button>
                    </children>
                    <BorderPane.margin>
                        <Insets left="10.0" top="10.0" />
                    </BorderPane.margin>
                    <padding>
                        <Insets top="10.0" />
                    </padding>
                </VBox>
            </left>
            <center>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
            </center>
        </BorderPane>
    </children>
</AnchorPane>
